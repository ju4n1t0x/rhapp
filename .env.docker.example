# Archivo: .env.docker.example
# Copia este archivo como .env.docker y configura tus valores

# Laravel App
APP_ENV=local
APP_DEBUG=true
APP_KEY=  # Genera una nueva con: php artisan key:generate

# Base de datos
DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=laravel
DB_PASSWORD=your_password_here

# MySQL
MYSQL_DATABASE=laravel
MYSQL_USER=laravel
MYSQL_PASSWORD=your_password_here
MYSQL_ROOT_PASSWORD=your_root_password_here